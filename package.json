{
  "name": "gerador-validador-cpf",
  "version": "3.1.3",
  "description": "Biblioteca JS open-source para gerar, validar e formatar CPF.",
  "homepage": "http://tiagoporto.github.io/gerador-validador-cpf",
  "keywords": [
    "cpf",
    "gerador",
    "generator",
    "generates",
    "validador",
    "valida",
    "validates",
    "formata",
    "format"
  ],
  "author": {
    "name": "Tiago Porto",
    "url": "http://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/gerador-validador-cpf.git"
  },
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "deploy": "npm run build && gh-pages -d build",
    "lint": "eslint ./src",
    "lint-fix": "npm run lint -- --fix",
    "cover": "nyc --reporter=lcov npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "test": "mocha './{,!(node_modules)/**/}*.test.js' test/**/*.js test/*.js",
    "test-watch": "npm test -- --watch",
    "precommit": "npm run lint",
    "tag": "git tag v$npm_package_version",
    "postpublish": "npm run tag && git push --tags"
  },
  "main": "dist/js/CPF.js",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "clipboard": "^2.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "highlightjs": "^9.10.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  }
}
